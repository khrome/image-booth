{
    "name": "image-booth",
    "homepage": "https://github.com/khrome/image-booth",
    "version": "0.0.1",
    "main": "image-booth.js",
    "module": "image-booth.mjs",
    "description": "A port of ImageBooth from Protolus with pluggable render engine and fewer deps.",
    "keywords": [
        "image",
        "processing"
    ],
    "author": "Abbey Hawk Sparrow <@khrome> (http://patternweaver.com)",
    "contributors": [],
    "bugs": {
        "url": "https://github.com/khrome/image-booth/issues"
    },
    "license": "MIT",
    "scripts": {
        "lint": "./node_modules/.bin/eslint *.js src/*.js test/*.js",
        "test": "./node_modules/.bin/mocha test/test.js",
        "build-commonjs": "./node_modules/.bin/babel src --out-dir dist",
        "browser-test": "open http://localhost:8000/test/test.html; npx http-server -p 8000",
        "demo": "open http://localhost:8000/test/demo.html; npx http-server -p 8000",
        "container-test": "docker build . -t submesh-treadmill -f ./containers/test.dockerfile; docker logs --follow \"$(docker run -d submesh-treadmill)\""
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:khrome/image-booth.git"
    },
    "dependencies": {
        "@hyperswarm/testnet": "^3.1.4",
        "array-events": "^1.1.0",
        "async-arrays": "^1.1.0",
        "canvas": "*",
        "environment-safe-canvas": "^2.0.0",
        "extended-emitter": "^1.3.1",
        "extended-emitter-es6": "^1.1.3",
        "file-saver": "^2.0.5",
        "hypercore": "^10.15.1",
        "hyperdrive": "^11.2.0",
        "hyperswarm": "^4.5.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.5",
        "@babel/core": "^7.22.5",
        "@babel/plugin-transform-modules-commonjs": "^7.22.5",
        "chai": "*",
        "environment-safe-chai": "^0.0.1",
        "mocha": "*",
        "wing-kong": "^0.0.1"
    },
    "engines": {
        "node": "*"
    }
}
